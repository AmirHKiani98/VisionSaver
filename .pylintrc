[MASTER]
ignore=__pycache__,migrations
jobs=1
load-plugins=
init-hook='import sys; sys.path.append("c:/MyApps/VisionSaver"); sys.path.append("c:/MyApps/VisionSaver/backend")'
[MESSAGES CONTROL]
disable=
    missing-docstring,
    invalid-name,
    too-few-public-methods,
    too-many-arguments,
    too-many-instance-attributes,
    too-many-locals,
    too-many-branches,
    too-many-statements,
    line-too-long,
    fixme
    no-member

[FORMAT]
max-line-length=120
indent-string='    '

[REPORTS]
output-format=colorized
reports=no

[TYPECHECK]
ignored-modules=cv2,torch,pandas,numpy,scipy,django,channels,asgiref

[DESIGN]
max-args=8
max-attributes=12

[LOGGING]
logging-modules=logging,logger

[VARIABLES]
dummy-variables-rgx=(_$|dummy|unused|ignore)

[SIMILARITIES]
min-similarity-lines=4